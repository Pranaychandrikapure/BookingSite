{
    "builds": [
        {
            "src": "manage.py",
            "use": "@vercel/python"
        },
        {
            "src": "vercel_wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_wsgi.py"
        }
    ]
}